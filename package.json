{
    "name": "unified-red",
    "version": "0.0.0",
    "description": "A set of nodes in Node-RED to create a customizable building automation user interface or HMI",
    "keywords": [
        "node-red"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "publishConfig": {
        "access": "public",
        "branches": [
            "master"
        ]
    },
    "files": [
        "api/*",
        "audit/*",
        "nodes/*",
        "src/*",
        "static/*",
        "admin-auth.js",
        "index.js",
        "LICENSE",
        "monitor.js",
        "socket.js",
        "ui.js",
        "dist/*",
        "api/config.json"
    ],
    "contributors": [
        {
            "name": "Nick Lee",
            "email": "nlee@wasocal.com"
        },
        {
            "name": "Samy Arbid",
            "email": "sarbid@wasocal.com"
        },
        {
            "name": "Michael Montalvo",
            "email": "mmontalvo@wasocal.com"
        }
    ],
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
